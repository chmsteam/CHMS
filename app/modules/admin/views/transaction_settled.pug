extends ../../../templates/navbaradmin.pug

block content
    section#services
        .right_col(role='main' style="background-color:transparent")
            .title_left
                h5
                    | Transaction / Settlement / 
                    b Settled Transaction
                .clearfix
                .row
                    .col-md-12.col-sm-12.col-xs-12
                        .x_panel(style='background-color:rgba(52,152,219,.3)')
                            br
                            .x_content
                            .x_panel
                                br
                                table#datatable.table.table-striped.table-bordered
                                    thead
                                        tr
                                            //- th ID
                                            th(style="text-align:center") NAME
                                            th(style="text-align:center") TYPE OF REQUEST
                                            th(style="text-align:center") DATE SETTLED
                                            th(style="text-align:center") TRANSACTION WILL END
                                            th(style="text-align:center") STATUS
                                            th(style="text-align:center") ACTION
                                    tbody
                                        each list in listtab
                                            tr(style="text-align:center")
                                                //- td #{list.intTRequestID}
                                                td(style="text-transform:capitalize") #{list.strName}
                                                td #{list.strRequestType}
                                                td #{list.datDateSettled}
                                                td #{list.dateexpire}
                                                td #{list.strTStatus}
                                                td 
                                                    a(href='transaction_settled_'+list.intTRequestID)
                                                        button.btn.btn-info(type='button')
                                                            i.fa.fa-eye
                                                            |   View